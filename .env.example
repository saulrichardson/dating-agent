# Concierge environment template
# Usage:
#   cp .env.example .env
#   set -a; source .env; set +a

# Service endpoints
CONTEXT_SERVICE_URL=http://localhost:8080

# Appium / Android tooling
APPIUM_SERVER_URL=http://127.0.0.1:4723
APPIUM_PORT=4723
NODE_VERSION=20
ANDROID_SDK_ROOT=/opt/homebrew/share/android-commandlinetools
ANDROID_HOME=/opt/homebrew/share/android-commandlinetools
CONCIERGE_ARTIFACTS_DIR=artifacts

# Optional credential placeholders.
# Note: current repo code does not auto-read these variables yet.
GOOGLE_TEST_EMAIL=
GOOGLE_TEST_PASSWORD=
HINGE_TEST_PHONE=
HINGE_TEST_EMAIL=
TINDER_TEST_PHONE=
TINDER_TEST_EMAIL=
OPENAI_API_KEY=
